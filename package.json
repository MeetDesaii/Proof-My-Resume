{
  "name": "shadcn-ui-monorepo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "start": "turbo start",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "clean": "turbo clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@visume/eslint-config": "workspace:*",
    "@visume/typescript-config": "workspace:*",
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.7.3",
    "@turbo/gen": "^2.5.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@tailwindcss/oxide",
      "core-js-pure",
      "sharp"
    ],
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "core-js-pure",
      "sharp"
